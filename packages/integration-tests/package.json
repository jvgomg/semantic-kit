{
  "name": "@webspecs/integration-tests",
  "version": "0.0.17",
  "private": true,
  "description": "Integration tests for webspecs packages",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "bun test --preload ./setup.ts .",
    "test:watch": "bun test --watch --preload ./setup.ts .",
    "lint": "eslint .",
    "pretty": "prettier --write .",
    "pretty:check": "prettier --check .",
    "clean": "rm -rf tsconfig.tsbuildinfo"
  },
  "devDependencies": {
    "@webspecs/cli": "workspace:*",
    "@webspecs/core": "workspace:*"
  }
}
